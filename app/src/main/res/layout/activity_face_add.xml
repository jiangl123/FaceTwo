<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.liu04.facetest.activity.FaceAddActivity">
    <ImageView
        android:id="@+id/image1"
        android:layout_width="150dp"
        android:onClick="onClick"
        android:background="#000000"
        android:layout_height="150dp"
        android:layout_margin="10dp"
        android:layout_alignParentLeft="true"
        tools:ignore="OnClick"/>
    <ImageView
        android:id="@+id/image2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="#000000"
        android:onClick="onClick"
        android:layout_margin="10dp"
        android:layout_alignParentRight="true"
        tools:ignore="OnClick"/>
    <Button
        android:id="@+id/btn1"
        android:layout_below="@+id/image1"
        android:layout_width="100dp"
        android:onClick="clickTo"
        android:text="人脸注册"
        android:layout_height="40dp"
        tools:ignore="OnClick"/>
    <TextView
        android:id="@+id/textResult"
        android:layout_below="@+id/edit_user_info"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <EditText
        android:id="@+id/edit_uid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="请输入uid"
        android:layout_margin="5dp"
        android:textSize="14sp"
        android:layout_below="@+id/btn1"/>
    <EditText
        android:id="@+id/edit_user_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="请输入user_info"
        android:layout_margin="5dp"
        android:textSize="14sp"
        android:layout_below="@+id/edit_uid"/>
</RelativeLayout>
